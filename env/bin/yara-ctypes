#!/Users/adam/ownCloud/Dev/Hamburglar/env/bin/python3
# EASY-INSTALL-ENTRY-SCRIPT: 'yara==1.7.7','console_scripts','yara-ctypes'
__requires__ = 'yara==1.7.7'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('yara==1.7.7', 'console_scripts', 'yara-ctypes')()
    )
